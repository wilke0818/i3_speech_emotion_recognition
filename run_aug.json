{
  "path_to_model_files": "./model/inputs/07262023",
  "output_path": "./model/final/07262023",
  "hp_amount_of_training_data": 0.3,
  "hp_num_trials": 5,
  "cross_validation": 5,
  "datasets": [
    {
      "name": "emovo",
      "eval_csv_path": "./data/emovo/"
    },
    {
      "name": "emozionalmente"
    }
  ],
  "augmentations": {
    "PitchShift": {
      "module": "torch_audiomentations",
      "class": "PitchShift",
      "sample_rate":16000,
      "output_type":"dict",
      "min_transpose_semitones":-3.0,
      "max_transpose_semitones":3.0,
      "p":1.0
    },
    "VolumeNormalize": {
      "module": "audiomentations",
      "class": "LoudnessNormalization",
      "min_lufs":-12.0, 
      "max_lufs":-12.0, 
      "p": 1.0,
      "tensors": false
    },
    "ImpulseResponse": {
      "module": "torch_audiomentations",
      "class": "ApplyImpulseResponse",
      "ir_paths":"om2/user/fabiocat/csad/data/audio/ir_folder/",
      "p":1.0,
      "p_mode":"per_example",
      "mode":"per_example",
      "sample_rate":16000,
      "output_type":"dict"
    },
    "BackgroundNoise": {
      "module": "torch_audiomentations",
      "class": "AddBackgroundNoise",
      "background_paths":"om2/user/fabiocat/csad/data/audio/backgrounds/augmentation/musan/",
      "output_type":"dict",
      "p":1.0,
      "sample_rate":16000,
      "p_mode":"per_example",
      "mode":"per_example",
      "min_snr_in_db":4.771,
      "max_snr_in_db":19.956
    },
    "ColoredNoise": {
      "module": "torch_audiomentations",
      "class": "AddColoredNoise",
      "output_type":"dict",
      "p":1.0,
      "sample_rate":16000,
      "p_mode":"per_example",
      "mode":"per_example",
      "min_snr_in_db":4.771,
      "max_snr_in_db":19.956,
      "min_f_decay":2,
      "max_f_decay":2
    },
    "TimeMask": {
      "module": "audiomentations",
      "class": "TimeMask",
      "p": 1.0,
      "min_band_part": 0.1,
      "max_band_part": 0.2,
      "tensors": false
    },
    "FrequencyMask": {
      "module": "custom_augmentations",
      "class": "FrequencyMask",
      "freq_mask_param": 50,
      "p":1.0,
      "tensors": false
    }
  }
}
